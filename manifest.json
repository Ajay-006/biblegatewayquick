{
  "manifest_version": 2,
  "name": "quick BibleGateway",
  "version": "1.1.0",
  "description": "Make quick searches in the Bible using biblegateway.com",
  "icons": {
    "16": "assets/icons/search_icon_16.png",
    "48": "assets/icons/search_icon_48.png",
    "128": "assets/icons/search_icon_128.png"
  },
  "permissions": [
    "menus",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "src/parser.js",
      "src/menu.js",
      "src/request.js",
      "src/error.js"
    ]
  }
}